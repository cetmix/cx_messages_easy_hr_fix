<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron that process some custom queue -->
        <record id="ir_cron_cetmix_sample_action" model="ir.cron">
            <field name="name">Cetmix Sample: Process some queue</field>
            <field name="model_id" ref="model_cetmix_sample_model"/>
            <field name="state">code</field>
            <field name="code">model._process_mass_mailing_queue()</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">60</field>
            <field name="interval_type">minutes</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')" />
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
        </record>
    </data>
</odoo>